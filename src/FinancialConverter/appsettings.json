{
  "CodaConverter": {
    "Mappings": [
      {
        "InType": "BNP",
        "InPath": "Fortis/Fortis-BVBA",
        "InExtension": "*.csv",

        "OutType": "ING",
        "OutPath": "Gripp/Fortis-BVBA"
      },
      {
        "InType": "BNP",
        "InPath": "Fortis/Fortis-BVBA",
        "InExtension": "*.csv",

        "OutType": "YNAB",
        "OutPath": "Ynab/Fortis-BVBA"
      },

      {
        "InType": "BNP",
        "InPath": "Fortis/Fortis-Private",
        "InExtension": "*.csv",

        "OutType": "YNAB",
        "OutPath": "Ynab/Fortis-Private"
      },

      {
        "InType": "Coda",
        "InPath": "Coda/BankVanBreda-BVBA",
        "InExtension": "*.cod",

        "OutType": "ING",
        "OutPath": "Gripp/BankVanBreda-BVBA"
      },
      {
        "InType": "Coda",
        "InPath": "Coda/BankVanBreda-BVBA",
        "InExtension": "*.cod",

        "OutType": "Ynab",
        "OutPath": "Ynab/BankVanBreda-BVBA"
      },

      {
        "InType": "Coda",
        "InPath": "Coda/BankVanBreda-BVBA-Savings",
        "InExtension": "*.cod",

        "OutType": "ING",
        "OutPath": "Gripp/BankVanBreda-BVBA-Savings"
      },
      {
        "InType": "Coda",
        "InPath": "Coda/BankVanBreda-BVBA-Savings",
        "InExtension": "*.cod",

        "OutType": "YNAB",
        "OutPath": "Ynab/BankVanBreda-BVBA-Savings"
      },

      {
        "InType": "Coda",
        "InPath": "Coda/BankVanBreda-Private",
        "InExtension": "*.cod",

        "OutType": "YNAB",
        "OutPath": "Ynab/BankVanBreda-Private"
      }
    ]
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "CODA to Gripp ING Converter"
    }
  }
}
